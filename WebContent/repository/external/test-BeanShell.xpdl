                    <Performer>projectmanager</Performer>
                    <StartMode>
                        <Automatic/>
                    </StartMode>
                    <FinishMode>
                        <Automatic/>
                    </FinishMode>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="ParticipantID" Value="projectmanager"/>
                        <ExtendedAttribute Name="XOffset" Value="250"/>
                        <ExtendedAttribute Name="YOffset" Value="60"/>
                    </ExtendedAttributes>
                </Activity>
                <Activity Id="programming" Name="Programming">
                    <Implementation>
                        <No/>
                    </Implementation>
                    <Performer>programmer</Performer>
                    <StartMode>
                        <Automatic/>
                    </StartMode>
                    <FinishMode>
                        <Automatic/>
                    </FinishMode>
                    <TransitionRestrictions>
                        <TransitionRestriction>
                            <Join Type="XOR"/>
                        </TransitionRestriction>
                    </TransitionRestrictions>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="ParticipantID" Value="programmer"/>
                        <ExtendedAttribute Name="XOffset" Value="350"/>
                        <ExtendedAttribute Name="YOffset" Value="70"/>
                    </ExtendedAttributes>
                </Activity>
                <Activity Id="test" Name="Test">
                    <Implementation>
                        <No/>
                    </Implementation>
                    <Performer>tester</Performer>
                    <StartMode>
                        <Automatic/>
                    </StartMode>
                    <FinishMode>
                        <Automatic/>
                    </FinishMode>
                    <TransitionRestrictions>
                        <TransitionRestriction>
                            <Split Type="XOR">
                                <TransitionRefs>
                                    <TransitionRef Id="test_Wor4_Tra5"/>
                                    <TransitionRef Id="test_Wor4_Tra4"/>
                                </TransitionRefs>
                            </Split>
                        </TransitionRestriction>
                    </TransitionRestrictions>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="ParticipantID" Value="tester"/>
                        <ExtendedAttribute Name="XOffset" Value="490"/>
                        <ExtendedAttribute Name="YOffset" Value="30"/>
                        <ExtendedAttribute Name="VariableToProcess_UPDATE" Value="status"/>
                    </ExtendedAttributes>
                </Activity>
                <Activity Id="release" Name="Release">
                    <Implementation>
                        <No/>
                    </Implementation>
                    <Performer>projectmanager</Performer>
                    <StartMode>
                        <Automatic/>
                    </StartMode>
                    <FinishMode>
                        <Automatic/>
                    </FinishMode>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="ParticipantID" Value="projectmanager"/>
                        <ExtendedAttribute Name="XOffset" Value="620"/>
                        <ExtendedAttribute Name="YOffset" Value="60"/>
                    </ExtendedAttributes>
                </Activity>
            </Activities>
            <Transitions>
                <Transition From="specification" Id="test_Wor4_Tra2" Name="Transition" To="programming">
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="RoutingType" Value="NOROUTING"/>
                        <ExtendedAttribute Name="BreakPoint" Value="283;234;1"/>
                    </ExtendedAttributes>
                </Transition>
                <Transition From="programming" Id="test_Wor4_Tra3" Name="Transition" To="test">
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="RoutingType" Value="NOROUTING"/>
                        <ExtendedAttribute Name="BreakPoint" Value="378;347;1"/>
                    </ExtendedAttributes>
                </Transition>
                <Transition From="test" Id="test_Wor4_Tra4" Name="Transition" To="programming">
                    <Condition Type="OTHERWISE"/>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="RoutingType" Value="NOROUTING"/>
                        <ExtendedAttribute Name="BreakPoint" Value="525;234;1"/>
                    </ExtendedAttributes>
                </Transition>
                <Transition From="test" Id="test_Wor4_Tra5" Name="Transition" To="release">
                    <Condition Type="CONDITION">status.equals("tested");</Condition>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="RoutingType" Value="NOROUTING"/>
                        <ExtendedAttribute Name="BreakPoint" Value="656;347;1"/>
                    </ExtendedAttributes>
                </Transition>
            </Transitions>
            <ExtendedAttributes>
                <ExtendedAttribute Name="StartOfWorkflow" Value="projectmanager;specification;110;60;NOROUTING"/>
                <ExtendedAttribute Name="EndOfWorkflow" Value="projectmanager;release;500;60;NOROUTING"/>
                <ExtendedAttribute Name="ParticipantVisualOrder" Value="projectmanager;programmer;tester;"/>
            </ExtendedAttributes>
