        <WorkflowProcess AccessLevel="PUBLIC" Id="manufacturing" Name="manufacturing">
            <ProcessHeader DurationUnit="D">
                <Created>30-Oct-2002</Created>
                <Description>Shows how the process flow is splited into two separated branches that are executed simultaniously. When both branches are joined, the process continues it's execution from the join point .</Description>
            </ProcessHeader>
            <RedefinableHeader PublicationStatus="UNDER_TEST">
                <Author>Sasa Bojanic</Author>
            </RedefinableHeader>
            <Participants>
                <Participant Id="worker1" Name="Worker 1">
                    <ParticipantType Type="ROLE"/>
                </Participant>
                <Participant Id="worker2" Name="Worker 2">
                    <ParticipantType Type="ROLE"/>
                </Participant>
                <Participant Id="composer" Name="Composer">
                    <ParticipantType Type="ROLE"/>
                </Participant>
                <Participant Id="administrator" Name="Administrator">
                    <ParticipantType Type="ROLE"/>
                </Participant>
            </Participants>
            <Activities>
                <Activity Id="receive_order" Name="receive order">
                    <Implementation>
                        <No/>
                    </Implementation>
                    <Performer>administrator</Performer>
                    <StartMode>
                        <Automatic/>
                    </StartMode>
                    <FinishMode>
                        <Automatic/>
                    </FinishMode>
                    <TransitionRestrictions>
                        <TransitionRestriction>
                            <Split Type="AND">
                                <TransitionRefs>
                                    <TransitionRef Id="test_Wor1_Tra2"/>
                                    <TransitionRef Id="test_Wor1_Tra3"/>
                                </TransitionRefs>
                            </Split>
                        </TransitionRestriction>
                    </TransitionRestrictions>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="ParticipantID" Value="administrator"/>
                        <ExtendedAttribute Name="XOffset" Value="220"/>
                        <ExtendedAttribute Name="YOffset" Value="70"/>
                    </ExtendedAttributes>
                </Activity>
                <Activity Id="produce_part1" Name="produce part1">
                    <Implementation>
                        <No/>
                    </Implementation>
                    <Performer>worker1</Performer>
                    <StartMode>
                        <Automatic/>
                    </StartMode>
                    <FinishMode>
                        <Automatic/>
                    </FinishMode>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="ParticipantID" Value="worker1"/>
                        <ExtendedAttribute Name="XOffset" Value="390"/>
                        <ExtendedAttribute Name="YOffset" Value="60"/>
                    </ExtendedAttributes>
                </Activity>
                <Activity Id="produce_part2" Name="produce part2">
                    <Implementation>
                        <No/>
                    </Implementation>
                    <Performer>worker2</Performer>
                    <StartMode>
                        <Automatic/>
                    </StartMode>
                    <FinishMode>
                        <Automatic/>
                    </FinishMode>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="ParticipantID" Value="worker2"/>
                        <ExtendedAttribute Name="XOffset" Value="390"/>
                        <ExtendedAttribute Name="YOffset" Value="70"/>
                    </ExtendedAttributes>
                </Activity>
                <Activity Id="compose" Name="compose">
                    <Implementation>
                        <No/>
                    </Implementation>
                    <Performer>composer</Performer>
                    <StartMode>
                        <Automatic/>
                    </StartMode>
                    <FinishMode>
                        <Automatic/>
                    </FinishMode>
                    <TransitionRestrictions>
                        <TransitionRestriction>
                            <Join Type="AND"/>
                        </TransitionRestriction>
                    </TransitionRestrictions>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="ParticipantID" Value="composer"/>
                        <ExtendedAttribute Name="XOffset" Value="580"/>
                        <ExtendedAttribute Name="YOffset" Value="60"/>
                    </ExtendedAttributes>
                </Activity>
            </Activities>
            <Transitions>
                <Transition From="receive_order" Id="test_Wor1_Tra2" Name="Transition" To="produce_part1">
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="RoutingType" Value="SIMPLEROUTING"/>
                    </ExtendedAttributes>
                </Transition>
                <Transition From="receive_order" Id="test_Wor1_Tra3" Name="Transition" To="produce_part2">
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="RoutingType" Value="SIMPLEROUTING"/>
                    </ExtendedAttributes>
                </Transition>
                <Transition From="produce_part2" Id="test_Wor1_Tra4" Name="Transition" To="compose">
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="RoutingType" Value="SIMPLEROUTING"/>
                    </ExtendedAttributes>
                </Transition>
                <Transition From="produce_part1" Id="test_Wor1_Tra5" Name="Transition" To="compose">
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="RoutingType" Value="SIMPLEROUTING"/>
                    </ExtendedAttributes>
                </Transition>
            </Transitions>
            <ExtendedAttributes>
                <ExtendedAttribute Name="StartOfWorkflow" Value="administrator;receive_order;120;70;NOROUTING"/>
                <ExtendedAttribute Name="EndOfWorkflow" Value="composer;compose;700;60;NOROUTING"/>
                <ExtendedAttribute Name="ParticipantVisualOrder" Value="worker1;administrator;worker2;composer;"/>
            </ExtendedAttributes>
        </WorkflowProcess>
