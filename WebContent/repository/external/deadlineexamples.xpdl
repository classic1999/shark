                <Transition From="do_something" Id="do_something_timeout" Name="Transition" To="timeout">
                    <Condition Type="EXCEPTION">SYNC_EXCEPTION</Condition>
                    <Description>Happens when the deadline time for activity "do_something" has passed</Description>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="RoutingType" Value="NOROUTING"/>
