                        <BasicType Type="STRING"/>
                    </DataType>
                </DataField>
            </DataFields>
            <Activities>
                <Activity Id="do_something" Name="Do something">
                    <Description>If this activity is not finished 15 sec after it is activated, the asynchronous deadline exception happens, and process goes to activity "Async timeout", but activity is still running. If it is not finished after 30 sec, it is terminated by the engine, and process goes to activity "Sync Timeout"</Description>
                    <Implementation>
                        <Tool Id="logger" Type="APPLICATION">
                            <ActualParameters>
                                <ActualParameter>"do_something"</ActualParameter>
                                <ActualParameter>"Executed before deadline"</ActualParameter>
                                <ActualParameter>logmsg</ActualParameter>
                            </ActualParameters>
                        </Tool>
                    </Implementation>
                    <Performer>somerole</Performer>
                    <StartMode>
                        <Manual/>
                    </StartMode>
                    <FinishMode>
                        <Automatic/>
                    </FinishMode>
                    <Deadline Execution="ASYNCHR">
                        <DeadlineCondition>var d=new java.util.Date();
d.setTime(ACTIVITY_ACTIVATED_TIME.getTime()+15000);
d;</DeadlineCondition>
                        <ExceptionName>ASYNC_EXCEPTION</ExceptionName>
                    </Deadline>
                    <Deadline Execution="SYNCHR">
                        <DeadlineCondition>var d=new java.util.Date();
d.setTime(ACTIVITY_ACCEPTED_TIME.getTime()+30000);
d;</DeadlineCondition>
                        <ExceptionName>SYNC_EXCEPTION</ExceptionName>
                    </Deadline>
                    <TransitionRestrictions>
                        <TransitionRestriction>
                            <Split Type="XOR">
                                <TransitionRefs>
                                    <TransitionRef Id="simple_sync_deadline_Tra3"/>
                                    <TransitionRef Id="do_something_async_timeout"/>
                                    <TransitionRef Id="do_something_sync_timeout"/>
                                </TransitionRefs>
                            </Split>
                        </TransitionRestriction>
                    </TransitionRestrictions>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="ParticipantID" Value="somerole"/>
                        <ExtendedAttribute Name="XOffset" Value="178"/>
                        <ExtendedAttribute Name="YOffset" Value="56"/>
                    </ExtendedAttributes>
