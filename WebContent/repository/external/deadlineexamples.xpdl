    <Applications>
        <Application Id="logger">
            <FormalParameters>
                <FormalParameter Id="activityId" Mode="IN">
                    <DataType>
                        <BasicType Type="STRING"/>
                    </DataType>
                    <Description>The Id of activity definition</Description>
                </FormalParameter>
                <FormalParameter Id="additional_log_msg" Mode="IN">
                    <DataType>
                        <BasicType Type="STRING"/>
                    </DataType>
                    <Description>Additional message for logging</Description>
                </FormalParameter>
                <FormalParameter Id="log" Mode="INOUT">
                    <DataType>
                        <BasicType Type="STRING"/>
                    </DataType>
                    <Description>Execution log</Description>
                </FormalParameter>
            </FormalParameters>
            <ExtendedAttributes>
                <ExtendedAttribute Name="ToolAgentClass" Value="org.enhydra.shark.toolagent.JavaScriptToolAgent"/>
                <ExtendedAttribute Name="Script" Value="log=log+&quot;\n&quot;+&quot;Activity for definition &quot;+activityId+&quot; is executed at &quot;+(new java.util.Date()).toString()+&quot; - &quot;+additional_log_msg;"/>
            </ExtendedAttributes>
        </Application>
        <Application Id="printLog">
            <FormalParameters>
                <FormalParameter Id="logmsg" Mode="IN">
                    <DataType>
                        <BasicType Type="STRING"/>
                    </DataType>
                </FormalParameter>
            </FormalParameters>
            <ExtendedAttributes>
                <ExtendedAttribute Name="ToolAgentClass" Value="org.enhydra.shark.toolagent.JavaScriptToolAgent"/>
                <ExtendedAttribute Name="Script" Value="java.lang.System.out.println(&quot;LOG MESSAGE=&quot;+logmsg);"/>
            </ExtendedAttributes>
        </Application>
