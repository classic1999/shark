                        <ExtendedAttribute Name="VariableToProcess_VIEW" Value="logmsg"/>
                    </ExtendedAttributes>
                </Activity>
                <Activity Id="print_log" Name="Print log">
                    <Implementation>
                        <Tool Id="printLog" Type="APPLICATION">
                            <ActualParameters>
                                <ActualParameter>logmsg</ActualParameter>
                            </ActualParameters>
                        </Tool>
                    </Implementation>
                    <StartMode>
                        <Automatic/>
                    </StartMode>
